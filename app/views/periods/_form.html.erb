<%= form_for([@project, @schedule, @period]) do |f| %>
<div class="row">
  <div class="field large-6 columns">
    <%= f.label :bcws %><br>
    <%= f.number_field :bcws %>
  </div>
  <div class="field large-6 columns">
  <%= f.label :bcwp %><br>
    <%= f.number_field :bcwp %>
  </div>
  <div class="field large-6 columns">
    <%= f.label :acwp %><br>
    <%= f.number_field :acwp %>
  </div>
</div>
<div class="actions">
  <%= f.submit %>
</div>

<% end %>