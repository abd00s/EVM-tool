<%= form_for([@project, @schedule]) do |f| %>
<div class="row">
  <div class="field large-6 columns">
    <%= f.label :start_date %><br>
    <%= f.date_field :start_date %>
  </div>
  <div class="field large-6 columns">
    <%= f.label :end_date %><br>
    <%= f.date_field :end_date %>
  </div>
</div>
<div class="actions">
  <%= f.submit %>
</div>

<% end %>